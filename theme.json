{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "settings": {
        "appearanceTools": true,
        "useRootPaddingAwareAlignments": true,
        "layout": {
            "contentSize": "720px",
            "wideSize": "1800px"
        },
        "color": {
            "custom": true,
            "customDuotone": false,
            "customGradient": false,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [],
            "gradients": [],
            "palette": [
                {
                    "slug": "primary",
                    "color": "#2563eb",
                    "name": "Primary"
                },
                {
                    "slug": "primary-dark",
                    "color": "#1d4ed8",
                    "name": "Primary Dark"
                },
                {
                    "slug": "secondary",
                    "color": "#64748b",
                    "name": "Secondary"
                },
                {
                    "slug": "tertiary",
                    "color": "#64748b",
                    "name": "Tertiary"
                },
                {
                    "slug": "accent",
                    "color": "#0ea5e9",
                    "name": "Accent"
                },
                {
                    "slug": "background",
                    "color": "#ffffff",
                    "name": "Background"
                },
                {
                    "slug": "background-secondary",
                    "color": "#f8fafc",
                    "name": "Background Secondary"
                },
                {
                    "slug": "foreground",
                    "color": "#1e293b",
                    "name": "Foreground"
                },
                {
                    "slug": "foreground-secondary",
                    "color": "#475569",
                    "name": "Foreground Secondary"
                },
                {
                    "slug": "border",
                    "color": "#e2e8f0",
                    "name": "Border"
                },
                {
                    "slug": "base",
                    "color": "#ffffff",
                    "name": "Base"
                },
                {
                    "slug": "base-2",
                    "color": "#f8fafc",
                    "name": "Base Secondary"
                },
                {
                    "slug": "contrast",
                    "color": "#1e293b",
                    "name": "Contrast"
                },
                {
                    "slug": "contrast-2",
                    "color": "#f1f5f9",
                    "name": "Contrast Secondary"
                },
                {
                    "slug": "contrast-3",
                    "color": "#e2e8f0",
                    "name": "Contrast Tertiary"
                },
                {
                    "slug": "success",
                    "color": "#22c55e",
                    "name": "Success"
                },
                {
                    "slug": "warning",
                    "color": "#f59e0b",
                    "name": "Warning"
                },
                {
                    "slug": "error",
                    "color": "#ef4444",
                    "name": "Error"
                }
            ]
        },
        "typography": {
            "customFontSize": true,
            "dropCap": false,
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFamily": "'IBM Plex Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, sans-serif",
                    "slug": "body",
                    "name": "Body (IBM Plex Sans)",
                    "fontFace": [
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-300.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "300",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-300-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-400.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "400",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-400-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "500",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-500.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "500",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-500-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "600",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-600.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "600",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-600-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-700.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Sans",
                            "fontWeight": "700",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-sans-700-italic.woff2"]
                        }
                    ]
                },
                {
                    "fontFamily": "'IBM Plex Serif', Georgia, 'Times New Roman', serif",
                    "slug": "heading",
                    "name": "Heading (IBM Plex Serif)",
                    "fontFace": [
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-300.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "300",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-300-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-400.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "400",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-400-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "500",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-500.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "500",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-500-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "600",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-600.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "600",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-600-italic.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-700.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Serif",
                            "fontWeight": "700",
                            "fontStyle": "italic",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-serif-700-italic.woff2"]
                        }
                    ]
                },
                {
                    "fontFamily": "'IBM Plex Mono', 'SF Mono', Consolas, 'Liberation Mono', Menlo, monospace",
                    "slug": "monospace",
                    "name": "Code (IBM Plex Mono)",
                    "fontFace": [
                        {
                            "fontFamily": "IBM Plex Mono",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-mono-400.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Mono",
                            "fontWeight": "500",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-mono-500.woff2"]
                        },
                        {
                            "fontFamily": "IBM Plex Mono",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "src": ["file:./assets/fonts/ibm-plex/ibm-plex-mono-700.woff2"]
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.875rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "1.125rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "1.5rem",
                    "name": "Extra Large"
                },
                {
                    "slug": "xx-large",
                    "size": "2rem",
                    "name": "2X Large"
                },
                {
                    "slug": "xxx-large",
                    "size": "2.5rem",
                    "name": "3X Large"
                }
            ]
        },
        "spacing": {
            "customSpacingSize": true,
            "spacingScale": {
                "steps": 7
            },
            "spacingSizes": [
                {
                    "slug": "10",
                    "size": "0.25rem",
                    "name": "1"
                },
                {
                    "slug": "20",
                    "size": "0.5rem",
                    "name": "2"
                },
                {
                    "slug": "30",
                    "size": "0.75rem",
                    "name": "3"
                },
                {
                    "slug": "40",
                    "size": "1rem",
                    "name": "4"
                },
                {
                    "slug": "50",
                    "size": "1.5rem",
                    "name": "5"
                },
                {
                    "slug": "60",
                    "size": "2rem",
                    "name": "6"
                },
                {
                    "slug": "70",
                    "size": "3rem",
                    "name": "7"
                },
                {
                    "slug": "80",
                    "size": "4rem",
                    "name": "8"
                }
            ],
            "units": ["px", "em", "rem", "%", "vw", "vh"]
        },
        "blocks": {
            "core/code": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)"
                }
            },
            "core/preformatted": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)"
                }
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--background)",
            "text": "var(--wp--preset--color--foreground)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--body)",
            "fontSize": "var(--wp--preset--font-size--medium)",
            "lineHeight": "1.6"
        },
        "spacing": {
            "padding": {
                "top": "var(--wp--preset--spacing--50)",
                "right": "var(--wp--preset--spacing--40)",
                "bottom": "var(--wp--preset--spacing--50)",
                "left": "var(--wp--preset--spacing--40)"
            }
        },
        "elements": {
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontWeight": "600",
                    "lineHeight": "1.3"
                },
                "color": {
                    "text": "var(--wp--preset--color--foreground)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xxx-large)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xx-large)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--primary-dark)"
                    }
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--background)"
                },
                "typography": {
                    "fontWeight": "500"
                },
                "border": {
                    "radius": "0.375rem"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--primary-dark)"
                    }
                }
            }
        },
        "blocks": {
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)",
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "700"
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--foreground)"
                        },
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--primary)"
                            }
                        }
                    }
                }
            },
            "core/navigation": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--body)",
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "core/post-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading)"
                }
            },
            "core/code": {
                "color": {
                    "background": "var(--wp--preset--color--background-secondary)",
                    "text": "var(--wp--preset--color--foreground)"
                },
                "border": {
                    "radius": "0.375rem"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--40)",
                        "right": "var(--wp--preset--spacing--40)",
                        "bottom": "var(--wp--preset--spacing--40)",
                        "left": "var(--wp--preset--spacing--40)"
                    }
                }
            },
            "core/quote": {
                "border": {
                    "left": {
                        "color": "var(--wp--preset--color--primary)",
                        "width": "4px"
                    }
                },
                "spacing": {
                    "padding": {
                        "left": "var(--wp--preset--spacing--50)"
                    }
                },
                "typography": {
                    "fontStyle": "italic"
                }
            }
        }
    },
    "customTemplates": [
        {
            "name": "page-no-title",
            "title": "Page (No Title)",
            "postTypes": ["page"]
        },
        {
            "name": "page-full-width",
            "title": "Page (Full Width)",
            "postTypes": ["page"]
        },
        {
            "name": "page-speaking",
            "title": "Speaking Page",
            "postTypes": ["page"]
        }
    ],
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        },
        {
            "name": "sidebar",
            "title": "Sidebar",
            "area": "uncategorized"
        }
    ],
    "patterns": []
}
